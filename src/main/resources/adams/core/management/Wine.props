# The wine executable for GUI applications
GUIExecutable=/usr/bin/wine

# Additional parameters for the GUIExecutable
GUIParameters=

# The wine executable for console applications
ConsoleExecutable=/usr/bin/wineconsole

# Additional parameters for the ConsoleExecutable
ConsoleParameters=--backend=user
