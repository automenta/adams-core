adams.flow.control.Flow
	-annotation
	This flow demonstrates how to process and access the elements\nof an array. Processing, as in converting the String array into a\nDouble array. Accessing elements, as in retrieving a value and\nwriting back another value.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.sink.Display
	-actor
		adams.flow.standalone.SetVariable
			-var-name
			index
			-var-value
			5
	-actor
		adams.flow.source.StringConstants
			-output-array
			true
			-string
			1
			-string
			2
			-string
			3
			-string
			4
			-string
			5
			-string
			6
			-string
			7
			-string
			8
			-string
			9
			-string
			10
	-actor
		adams.flow.control.ArrayProcess
			-actor
				adams.flow.transformer.Convert
					-conversion
						adams.data.conversion.StringToDouble
			-array-class
			java.lang.Double
	-actor
		adams.flow.control.Tee
			-name
			before
			-tee
				adams.flow.transformer.GetArrayElement
					-index
					@{index}
			-tee
				adams.flow.transformer.Convert
					-conversion
						adams.data.conversion.AnyToString
			-tee
				adams.flow.transformer.StringReplace
					-find
					^
					-replace
					before: 
			-tee
				adams.flow.sink.CallableSink
					-callable
					Display
	-actor
		adams.flow.transformer.SetArrayElement
			-index
			5
			-value
			5.5
	-actor
		adams.flow.control.Tee
			-name
			after
			-tee
				adams.flow.transformer.GetArrayElement
					-index
					@{index}
			-tee
				adams.flow.transformer.Convert
					-conversion
						adams.data.conversion.AnyToString
			-tee
				adams.flow.transformer.StringReplace
					-find
					^
					-replace
					after: 
			-tee
				adams.flow.sink.CallableSink
					-callable
					Display
	-actor
		adams.flow.transformer.ArrayToSequence
	-actor
		adams.flow.sink.CallableSink
			-callable
			Display
	-flow-execution-listener
		adams.flow.execution.NullListener

